package hw.ch4.enumCase;

enum colorType {
	Red,Green,blue
}

enum clorType2{
	Red("t色"),Green("G色"),blue("{色")
}

public class enumCase {
	
	public static void main(String[] args) {

		System.out.println(colorType.blue);
		System.out.println(colorType2.blue);
	}
}

//解enum和	Enum的^e
//enum 是一PI字，使用enum

